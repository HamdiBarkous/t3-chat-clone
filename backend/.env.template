# Supabase Configuration
SUPABASE_URL=supabase_url
SUPABASE_ANON_KEY=supabase_anon_key

# Database (Supabase PostgreSQL)
SUPABASE_SERVICE_ROLE_KEY=supabase_service_role_key

# MCP (Model Context Protocol) Configuration
MCP_ENABLED=true
MCP_SUPABASE_READ_ONLY=false

# Supabase MCP Server Configuration
# Personal access token for Supabase MCP tools (create at: https://supabase.com/dashboard/account/tokens)
SUPABASE_ACCESS_TOKEN=sbp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# Optional: Scope MCP server to specific project (get from project settings)
SUPABASE_PROJECT_REF=your_project_ref_id

# OpenRouter API
OPENROUTER_API_KEY=sk-or-v1-...